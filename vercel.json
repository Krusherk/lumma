{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/discord",
      "destination": "https://discord.gg/replace-with-code",
      "permanent": false
    },
    {
      "source": "/x",
      "destination": "https://x.com/lumma_xyz",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}

